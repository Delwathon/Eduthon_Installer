{
    "name": "delwathon/eduthon-installer",
    "description": "Eduthon product installer",
    "license": "MIT",
    "authors": [
        {
            "name": "Delwathon IT Solutions",
            "email": "dev@delwathon.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Delwathon\\LaravelInstaller\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Delwathon\\LaravelInstaller\\Providers\\LaravelInstallerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}